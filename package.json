{
  "name": "triage-trainer-react",
  "version": "0.1.0",
  "description": "Triage training simulator for combat medical instruction.",
  "author": "Aaron M. Bleuer",
  "homepage": "./",
  "main": "main.js",
  "reactScriptsVersion": "overridden",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.4"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "start": "npm run type-check && react-scripts start",
    "build": "npm run type-check && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "build-data": "tsc src/data/injuryProfiles.ts --outDir src/data/js --esModuleInterop",
    "split": "npm run build-data && node split-injury-profiles.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/uuid": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^33.0.0",
    "electron-builder": "^26.0.12",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
